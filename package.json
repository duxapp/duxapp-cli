{
  "name": "duxapp-cli",
  "version": "2.5.9",
  "description": "duxapp CLI工具",
  "type": "module",
  "main": "./cjs/index.cjs",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./cjs/index.cjs"
    },
    "./src/*": {
      "import": "./src/*.js",
      "require": "./cjs/*.cjs"
    },
    "./cjs/*": "./cjs/*.cjs"
  },
  "directories": {
    "test": "test"
  },
  "files": [
    "appTemplate/",
    "scripts/",
    "src/",
    "cjs/",
    "README.md"
  ],
  "bin": {
    "duxapp": "./src/index.js"
  },
  "scripts": {
    "duxapp": "./src/index.js",
    "test": "node ./src/index.js",
    "build": "node ./scripts/build.js"
  },
  "author": "duxapp",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.27.5",
    "@xmldom/xmldom": "^0.9.8",
    "archiver": "^7.0.1",
    "bash-parser": "^0.5.0",
    "fs-extra": "^11.3.0",
    "inquirer": "^12.6.3",
    "jsdoc-api": "^9.3.4",
    "node-stream-zip": "^1.15.0",
    "object.omit": "^3.0.0",
    "plist": "^3.1.0",
    "request": "^2.88.2",
    "xcode": "^3.0.1",
    "xml-formatter": "^3.6.6"
  },
  "devDependencies": {
    "esbuild": "^0.25.5",
    "eslint": "^8.57.1"
  }
}
